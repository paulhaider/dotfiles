Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false

URxvt.saveLines: 1000
URxvt.scrollBar: false
URxvt.foreground: white
URxvt.background: black
URxvt.secondaryScroll: true
URxvt.depth: 32
URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10
URxvt.boldFont: xft:DejaVu Sans Mono for Powerline:bold:size=10
!URxvt.font: xft:Inconsolata for Powerline:size=12
!URxvt.boldFont: xft:Inconsolata:bold:size=12
!URxvt.font: xft:Terminus:style=Regular:size=12
!URxvt.boldFont: xft:Terminus:style=Bold:size=12
!URxvt.font: xft:Anonymous Pro:style=Regular:size=11
!URxvt.boldFont: xft:Anonymous Pro:style=Bold:size=11
!URxvt*letterSpace: -1
URxvt.perl-ext-common: default,matcher,fullscreen,kuake,clipboard
URxvt.kuake.hotkey: F12
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.urlLauncher: chromium
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt*inheritPixmap: false
URxvt*shading: 70

! for numpad to work
URxvt.keysym.KP_Add:    +
URxvt.keysym.KP_Subtract: -
URxvt.keysym.KP_Multiply: *
URxvt.keysym.KP_Divide  : /
URxvt.keysym.KP_Decimal : .
URxvt.keysym.KP_Separator : ,
URxvt.keysym.KP_1:      1
URxvt.keysym.KP_2:      2
URxvt.keysym.KP_3:      3
URxvt.keysym.KP_4:      4
URxvt.keysym.KP_5:      5
URxvt.keysym.KP_6:      6
URxvt.keysym.KP_7:      7
URxvt.keysym.KP_8:      8
URxvt.keysym.KP_9:      9
URxvt.keysym.KP_0:      0

! for jumping to beginning/end of line keys to work
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

       output eDP-1 enable mode 1920x1080 position 0,0
}

profile {
       output eDP-1 disable
       output DP-1 mode 2560x1440@59.951Hz position 1920,0
}

profile {
       output eDP-1 disable
       output HDMI-A-2 mode 2560x1440@59.951Hz position 0,0
}

profile {
       output eDP-1 disable
       output HDMI-A-2 mode 1920x1080@50.000Hz position 1920,0
}
